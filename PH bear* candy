local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/PitchyATree/roblox-exploit-library/main/turtle%20library%20red%20version"))()
local window = library:Window("PitchHub - CandyFarm")
window:Label("player can saw candy on you , be aware ! ", Color3.fromRGB(127, 143, 166))
window:Label("i'm not responsible if you guys get banned", Color3.fromRGB(127, 143, 166))
window:Toggle("Bucket ", false, function(bool)  -- you can change window to the ui you want the toggle in 
     getgenv().Bucket = bool
     while wait() do
         if getgenv().Bucket == true then
             local me = game.Players.LocalPlayer.Character.HumanoidRootPart
for i,v in pairs(game.Workspace.Candy_Bin:GetDescendants()) do
if v.Name == "Bucket" then
v.CFrame = me.CFrame
end
end
end
end-- bool is true or false depending on the state of the toggle
end)
