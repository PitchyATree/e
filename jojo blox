--Properties:

cool.Name = "cool"
cool.Parent = game.CoreGui
cool.ZIndexBehavior = Enum.ZIndexBehavior.Sibling

OpenFrame.Name = "OpenFrame"
OpenFrame.Parent = cool
OpenFrame.Active = true
OpenFrame.BackgroundColor3 = Color3.fromRGB(112, 123, 199)
OpenFrame.Position = UDim2.new(0.00739176152, 0, 0.512618303, 0)
OpenFrame.Size = UDim2.new(0, 120, 0, 39)

Open.Name = "Open"
Open.Parent = OpenFrame
Open.BackgroundColor3 = Color3.fromRGB(255, 255, 255)
Open.BackgroundTransparency = 1.000
Open.Position = UDim2.new(-0.0916666687, 0, -0.103433333, 0)
Open.Size = UDim2.new(0, 141, 0, 46)
Open.Font = Enum.Font.Cartoon
Open.Text = "Open"
Open.TextColor3 = Color3.fromRGB(255, 255, 255)
Open.TextSize = 35.000
Open.TextTransparency = 0.100
Open.MouseButton1Down:connect(function()
Main.Visible = true
OpenFrame.Visible = false
end)


