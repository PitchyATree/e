ItalyTP.Name = "Italy TP"
ItalyTP.Parent = Main
ItalyTP.BackgroundColor3 = Color3.fromRGB(0, 0, 0)
ItalyTP.BackgroundTransparency = 0.500
ItalyTP.BorderColor3 = Color3.fromRGB(48, 0, 0)
ItalyTP.BorderSizePixel = 3
ItalyTP.Position = UDim2.new(0.0367101952, 0, 0.0928271413, 0)
ItalyTP.Size = UDim2.new(0, 100, 0, 21)
ItalyTP.Font = Enum.Font.Cartoon
ItalyTP.Text = "Italy TP"
ItalyTP.TextColor3 = Color3.fromRGB(144, 255, 26)
ItalyTP.TextSize = 20.000
ItalyTP.TextStrokeTransparency = 0.000
ItalyTP.MouseButton1Down:connect(function()
placeid = 4313854107  --change this to the place id you want
tp = game:GetService('TeleportService')

for i,v in pairs (game.Players:GetChildren()) do
    tp:Teleport(placeid, v)
end
end)

