-- Scripts:

local function DAMI_fake_script() -- TextLabel.LocalScript 
	local script = Instance.new('LocalScript', TextLabel)

	while true do
		script.Parent.Text = "hi"
		wait(1)
		script.Parent.Text = "close the ui won't break the autofarm"
		wait(10)
		script.Parent.Text = "but if it miss a token then you need to execute again"
		wait(12)
	end
	
	
end
coroutine.wrap(DAMI_fake_script)()
local function SUZFGZZ_fake_script() -- TextButton_2.Script 
	local script = Instance.new('Script', TextButton_2)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Parent.Visible = false
	end)
	
end
coroutine.wrap(SUZFGZZ_fake_script)()
